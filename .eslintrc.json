{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "react/no-unknown-property": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}